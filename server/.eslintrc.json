{
    "env": {
        "node": true,
        "es2021": true,
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": 0,
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
    }
}
